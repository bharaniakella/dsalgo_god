      "source": [
        "import numpy as np\n",
        "\n",
        "arr=np.array([4,2,8,5,1])\n",
        "\n",
        "def find_max(arr):\n",
        "  max=arr[0]\n",
        "  for i in range(1,len(arr)):\n",
        "    if(arr[i]>max):\n",
        "      max=arr[i]\n",
        "\n",
        "  return max"
      ]
    },
